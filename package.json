  "dependencies": {
    "@astrojs/mdx": "^4.0.3",
    "@astrojs/rss": "^4.0.10",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/solid-js": "^5.0.4",
    "@deno/astro-adapter": "^0.2.0",
    "@rocicorp/zero": "^0.11.2025011402",
    "astro": "^5.1.1",
    "motion": "^11.16.0",
    "solid-js": "^1.9.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1"
  }
